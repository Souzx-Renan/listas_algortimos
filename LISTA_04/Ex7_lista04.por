programa {
  funcao inicio() {
    inteiro n1, n2, n3
  
    escreva("Digite três valores: ")
    leia(n1, n2, n3)

    se (n1 < n2 e n1 < n3)
      escreva("O menor valores entre os três digitos = ", n1)
    senao se(n2 < n1 e n2 < n3)
      escreva("O menor valores entre os três digitos = ", n2)
    senao
      escreva("O menor valores entre os três digitos = ", n3)

    

    
  }
}
